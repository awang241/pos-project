<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.Dialog?>
<Dialog fx:id="dialog" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.dialog.CashInputDialogController">
   <dialogPane>
      <DialogPane onKeyReleased="#cashInputKeyPressHandler" prefHeight="150.0" prefWidth="182.0" xmlns="http://javafx.com/javafx/11.0.1">
         <content>
            <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="90.0" prefWidth="182.0">
               <children>
                  <TextField fx:id="cashInput" alignment="CENTER_RIGHT" layoutX="14.0" layoutY="14.0" mouseTransparent="true" prefHeight="33.0" prefWidth="156.0">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
               </children>
            </Pane>
         </content>
         <header>
            <Label onKeyReleased="#cashInputKeyPressHandler" prefHeight="41.0" prefWidth="122.0" text="Enter Cash Amount:">
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </header>
      </DialogPane>
   </dialogPane>
</Dialog>
